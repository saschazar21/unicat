{
  "name": "@saschazar/unicat",
  "version": "0.0.1",
  "private": false,
  "author": "Sascha Zarhuber <sascha.zarhuber@gmail.com>",
  "main": "index.min.js",
  "browser": "index.umd.min.js",
  "umd:main": "index.umd.min.js",
  "module": "index.es.min.js",
  "typings": "index.d.ts",
  "scripts": {
    "build:clean": "shx rm -rf dist",
    "build:compile": "rollup -c",
    "build:copy:styles": "shx mkdir dist/styles && shx cp -R src/__styles__/* dist/styles/",
    "build:copy": "shx cp package.json README.md LICENSE.md CHANGELOG.md dist/ && npm run build:copy:styles",
    "build:dir": "shx mkdir dist",
    "build": "npm run build:clean && npm run build:dir && npm run build:copy && npm run build:compile",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "styleguide:watch": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest --coverage",
    "test:update": "jest -u --coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@saschazar/unicat-icons": "^0.0.3",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.3",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.17",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "builtin-modules": "^3.1.0",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.0.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-animate-height": "^2.0.16",
    "react-docgen-typescript": "^1.12.4",
    "react-dom": "^16.8.6",
    "react-styleguidist": "^9.1.2",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.1",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.1",
    "typescript": "^3.4.5",
    "webpack": "^4.32.2"
  },
  "peerDependencies": {
    "@saschazar/unicat-icons": "^0.0.3",
    "classnames": "^2.2.6",
    "react": "^16.8.6",
    "react-animate-height": "^2.0.16"
  }
}
