{
  "name": "@saschazar/unicat",
  "version": "0.1.0",
  "private": false,
  "author": "Sascha Zarhuber <sascha.zarhuber@gmail.com>",
  "main": "index.min.js",
  "browser": "index.umd.min.js",
  "umd:main": "index.umd.min.js",
  "module": "index.es.min.js",
  "typings": "index.d.ts",
  "scripts": {
    "build:clean": "shx rm -rf dist",
    "build:compile": "rollup -c",
    "build:copy": "shx cp package.json dist/ && cp README.md dist/ && shx cp LICENSE.md dist/ && shx cp CHANGELOG.md dist/",
    "build:dir": "shx mkdir dist",
    "build": "npm run build:clean && npm run build:dir && npm run build:copy && npm run build:compile"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.17",
    "builtin-modules": "^3.1.0",
    "classnames": "^2.2.6",
    "node-sass": "^4.12.0",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.21.1",
    "shx": "^0.3.2",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.6"
  }
}
